<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Aishopssh.Entites.AdminReply" table="tb_admin_reply" schema="e_shopwebsite">
        <id name="id" column="ID"/>
        <property name="leavewordId" column="leavewordID"/>
        <property name="title" column="Title"/>
        <property name="content" column="Content"/>
        <property name="replyTime" column="ReplyTime"/>
        <property name="isRead" column="Is_Read"/>
    </class>
</hibernate-mapping>