<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Aishopssh.Entites.Cartlist" table="tb_cartlist" schema="e_shopwebsite">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="goodsNum" column="GoodsNum"/>
        <property name="goodsAddTime" column="GoodsAddTime"/>
        <property name="orderNum" column="OrderNum"/>
        <many-to-one name="tbGoodsByGoodsId" class="com.Aishopssh.Entites.Goods">
            <column name="GoodsID"/>
        </many-to-one>
        <many-to-one name="tbUserByUserId" class="com.Aishopssh.Entites.User">
            <column name="UserID"/>
        </many-to-one>
    </class>
</hibernate-mapping>